<div class="container">
  <h2>Adding new member</h2>
  <%= form_for(@user, url:{controller:'users', action:'create'}) do |form| %>
    <div class="form-group">
      <label for="username">Username</label>
      <%= form.text_field :username,{class:"form-control", placeholder:"username"} %>
      <div class="form-group">
      <label for="email">Email</label>
      <%= form.text_field :email,{class:"form-control", placeholder:"email"} %>
    <%= form.submit "作成" , class:"btn btn-primary" %>
    </div>
  <% end %>
  
</div>